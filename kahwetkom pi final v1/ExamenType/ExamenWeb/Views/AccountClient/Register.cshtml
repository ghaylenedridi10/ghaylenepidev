                                @Html.EditorFor(model => model.BirthDate, new { htmlAttributes = new { @class = "form-control" } })
                                @Html.ValidationMessageFor(model => model.BirthDate, "", new { @class = "text-danger" })
                            </div>
                        </div>
                        <br />
                        <div class="form-group">
                            <div class="col-md-offset-2 col-md-10">
                                <input type="submit" value="Create" class="testaa" />
                            </div>
                        </div>



                    </form>
                </div>
            </div>
        }

    </div>
</body>
</html>
<script src=@Url.Content("/Content/FrontOffice/js/jquery-3.2.1.min.js")></script>
<script src=@Url.Content("/Content/FrontOffice/styles/bootstrap4/popper.js")></script>
